{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": ["@types/react-native-dotenv"],
    "paths": {
      "@/*": ["./*", "src/*"],
      "react-native-svg": ["./node_modules/react-native-svg"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
  "exclude": ["node_modules"]
}
